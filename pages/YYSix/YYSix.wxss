/* 控制 */
.center {
    margin-top: 50rpx;
    margin-left: 26rpx;
    margin-right: 26rpx;
}

.efforts {
    width: 292rpx;
    border-radius: 10rpx;
    padding: 24rpx;
    text-align: left;
    display: inline-block;
    position: relative;
}

.degree {
    width: 340rpx;
    border-radius: 10rpx;
    text-align: left;
    display: inline-block;
    padding-bottom: 34rpx;
    overflow: hidden;
}

.degree .title {
    color: #ffffff;
    margin-left: 24rpx;
    margin-top: 24rpx;
}

.degree .text {
    height: 88rpx;
    width: 100%;
    font-family: PingFang SC;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #ffffff;
    opacity: 0.7;
    position: relative;
    display: block;
    margin-left: 24rpx;
}

.degree .level {
    font-size: 28rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    width: 88rpx;
    line-height: 88rpx;
    text-align: center;
}

.scroll {
    display: block;
    padding: 0 34rpx;
}



.degree .line {
    width: 293rpx;
    height: 10rpx;
    background-color: #6d7185;
    border-radius: 6rpx;
    margin-top: 75rpx;
    margin-bottom: 12rpx;
    position: relative;
    margin-left: 24rpx;
}

.srollDegree {
    height: 55rpx;
    width: 340rpx;
    position: absolute;
    bottom: 6rpx;
}

.inside {
    width: 589rpx;
}

.showLine {
    background-color: #65d9ff;
    width: 298rpx;
    height: 12rpx;
    margin-top: 15rpx;
    position: relative;
    margin-right: 293rpx;
}

.degreeBoll {
    width: 50rpx;
    height: 40rpx;
    position: absolute;
    top: -22rpx;
    right: -14rpx;
    background-color: rgba(0, 0, 0, 0);
    padding-top: 10rpx;
}

.insideBoll {
    width: 15rpx;
