.container {
    background-image: linear-gradient(181deg, #f2ffe4 0%, #f9ffec 100%);
}

.YYThree {
    overflow: auto;
    margin-top: 12rpx;
}

.instructionsBox {
    margin-top: 20rpx;
    margin-right: 36rpx;
    width: 88rpx;
}

.instructionsIMG {
    width: 80rpx;
    height: 80rpx;
    margin-left: 4rpx;
}

.instructionsTxt {
    width: 88rpx;
    font-family: PingFang SC;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #333;
    text-align: center;
    vertical-align: top;
}


.BigRoll {
    width: 616rpx;
    height: 616rpx;
    margin-left: 67rpx;
    position: relative;
    overflow: auto;
}

.BigRoll .numBox {
    width: 374rpx;
    height: 374rpx;
    background-color: #ffffff;
    box-shadow: 0rpx 8rpx 40rpx 0rpx rgba(137, 204, 61, 0.3);
    border-radius: 200rpx;
    margin-top: 121rpx;
    margin-left: 121rpx;
    position: relative;
    overflow: auto;
    z-index: 99;
}

.BigRoll .numBox .num {
    font-family: SF Pro Display;
    font-size: 154rpx;
    line-height: 184rpx;
    text-align: center;
    color: #333;
}

.BigRoll .numBox .unit {
    display: inline-block;
    font-family: PingFang SC;
    font-size: 40rpx;
    color: #7a5a5a;
    opacity: 0.9;
    position: absolute;
    top: 133rpx;
    right: 65rpx;
}

.BigRoll .numBox .txt {
    font-family: PingFang SC;
    font-size: 28rpx;
    color: #333;
    text-align: center;
    vertical-align: top;
    margin-top: 63rpx;
}

.BigRoll_bg {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}

.BigRoll_side {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}

.BigRoll_plus {
    width: 80rpx;
    height: 80rpx;
    margin-left: 200rpx;
}

.BigRoll_subtract {
    width: 80rpx;
    height: 80rpx;
    margin-right: 200rpx;
}

.timerBox {
    margin-top: 30rpx;
    height: 181rpx;
    position: relative;
}

.timerBox .choseBox {
    width: 80rpx;
    height: 80rpx;
    background-color: #fff;
    box-shadow: 0rpx 8rpx 40rpx 0rpx rgba(137, 204, 61, 0.3);
    border-radius: 16rpx;
    border: solid 2rpx #89cc3d;
    position: absolute;
    z-index: 99;
    top: 105rpx;
    font-family: SF Pro Display;
	font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
	letter-spacing: 0rpx;
	color: #89cc3d;
}

.timerBox .title {
    margin-left: 36rpx;
}

.timerBox .title .big {
    width: 56rpx;
    height: 40rpx;
    font-family: PingFang SC;
    font-size: 28rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 40rpx;
    letter-spacing: 0px;
    color: #333;
}


.timerBox .title .small {
    width: 59rpx;
    height: 30rpx;
    font-family: PingFang SC;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 30rpx;
    letter-spacing: 0px;
    color: #333;
    opacity: 0.5;
}

.timerBox .timerLine {
    margin-left: 36rpx;
    margin-top: 20rpx;
    font-family: SF Pro Display;
    font-size: 24rpx;
    line-height: 29rpx;
    letter-spacing: 0rpx;
    color: #333333;
    opacity: 0.5;
}

.timerBox .timerBar {
    margin-top: -20rpx;
    width: 100%;
    height: 150rpx;
    display: block;
}

.power {
    display: block;
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    margin-top: 50rpx;
}