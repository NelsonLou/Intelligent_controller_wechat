.container {
    background-image: linear-gradient(181deg,
            #fef7ee 0%,
            #ffb9ae 100%);
}

.YYEight {
    overflow: auto;
    margin-top: 12rpx;
}

.instructionsBox {
    margin-top: 20rpx;
    margin-right: 36rpx;
    width: 88rpx;
}

.instructionsIMG {
    width: 80rpx;
    height: 80rpx;
    margin-left: 4rpx;
}

.instructionsTxt {
    width: 88rpx;
    font-family: PingFang SC;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #666464;
    text-align: center;
    vertical-align: top;
}

.power {
    display: block;
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    margin-top: 100rpx;
}

/* 顶部 */

.topBox {
    margin-top: 40rpx;
}

.modelBox {
    width: 300rpx;
    height: 300rpx;
    position: relative;
    display: inline-block;
    margin-left: 24rpx;
}

.modelItem {
    width: 150rpx;
    height: 150rpx;
    display: inline-block;
    vertical-align: top;
    font-size: 30rpx;
}

.modelItem text {
    display: inline-block;
}

.unChose {
    background-color: #fff;
    color: #ff8370;
}

.chose {
    background-image: linear-gradient(0deg,
            #ffc2b8 0%,
            #ff8571 100%);
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(255, 131, 112, 0.5);
    color: #fff;
}

.lineCenter {
    background-color: #65d9ff;
    width: 100%;
    border-radius: 100rpx;
}

.modelBox .bg {
    width: 300rpx;
    height: 300rpx;
    position: absolute;
    left: 0;
    right: 0;
}

/* 力度 */
.degreeBox {
    width: 380rpx;
}

.degree,
.efforts {
    width: 292rpx;
    border-radius: 10rpx;
    padding: 12rpx 24rpx;
    text-align: left;
    background-color: #fff;
}

.efforts {
    margin-top: 20rpx;
}

.degree .text {
    font-family: PingFang SC;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 30rpx;
    letter-spacing: 0rpx;
    opacity: 0.7;
    margin-top: 16rpx;
    margin-bottom: 30rpx;
}

.degree .line {
    width: 293rpx;
    height: 12rpx;
    background-color: #ffe6e2;
    border-radius: 6rpx;
    margin-top: 24rpx;
    margin-bottom: 12rpx;
    position: relative;
}

.line .showLine {
    background-color: #ff8370;
    border-radius: 6rpx;
    height: 100%;
}

.line .boll {
    width: 10rpx;
    height: 10rpx;
    background-color: #ff8370;
    border: solid 10rpx #ffffff;
    border-radius: 20rpx;
    position: absolute;
    top: -10rpx;
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(255, 131, 112, 0.5);
}

.controler .item {
    overflow: auto;
    width: 80rpx;
    font-size: 24rpx;
    display: inline-block;
    text-align: center;
    vertical-align: top;
}

.controler .item .text {
    margin-top: 20rpx;
    height: 30rpx;
}

.effortsBtn {
    width: 80rpx;
    height: 80rpx;
    display: block;
    margin-bottom: 20rpx;
}

/* 底部 */


.timerBox {
    height: 181rpx;
    margin: 0 24rpx;
    margin-top: 60rpx;
    position: relative;
    border-radius: 16rpx;
    background-color: #fff;
    color: #333;
    padding-bottom: 50rpx;
    overflow: auto;
}

.timerBox .title {
    margin-top: 24rpx;
    margin-left: 24rpx;
}


.timerBox .title .big {
    width: 56rpx;
    height: 40rpx;
    font-family: PingFang SC;
    font-size: 28rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 40rpx;
    letter-spacing: 0px;
}

.timerBox .title .small {
    width: 59rpx;
    height: 30rpx;
    font-family: PingFang SC;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 30rpx;
    letter-spacing: 0px;
    opacity: 0.5;
}


.timerMain {
    margin-top: 24rpx;
    position: absolute;
    margin-left: 17rpx;
}

.timerItem {
    width: 48rpx;
    height: 117rpx;
    font-family: SF Pro Display;
    font-size: 24rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 29rpx;
    letter-spacing: 0px;
    text-align: center;
    position: relative;
}

.timerItem .boll {
    display: inline-block;
    background-color: #ffd3cc;
    border-radius: 20rpx;
    height: 39rpx;
    width: 39rpx;
    position: absolute;
    bottom: 5rpx;
    left: 5rpx;
    z-index: 999;
}

.marginleft {
    margin-left: 73rpx;
}

.bollTimer {
    width: 80rpx;
    height: 80rpx;
    margin-left: -20rpx;
    margin-top: -20rpx;
}

.timerLine {
    width: 598rpx;
    height: 24rpx;
    position: absolute;
    background-color: #ffe9e6;
    left: 40rpx;
    top: 82rpx;
}