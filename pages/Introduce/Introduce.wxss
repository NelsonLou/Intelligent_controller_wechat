.container {
    background-image: linear-gradient(181deg, #af56d5 0%, #050091 100%);
}

.Introduce {
    overflow: auto;
    margin-top: 12rpx;
}

.part {
    width: 590rpx;
    background-color: #ffffff;
    box-shadow: 0px 20rpx 40rpx 0rpx rgba(51, 51, 51, 0.1);
    border-radius: 32rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    margin-bottom: 10rpx;
    padding: 40rpx;
}

.title {
    font-family: PingFang SC;
    font-size: 36rpx;
    color: #333333;
}

.main {
    margin-top: 24rpx;
    overflow: auto;
    font-family: PingFang SC;
    font-size: 34rpx;
    line-height: 40rpx;
    color: #666666;
}