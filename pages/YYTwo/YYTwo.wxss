.timer {
    width: 718rpx;
    background-image: linear-gradient(0deg, #ffffff 0%, rgba(255, 255, 255, 0.34) 100%);
    box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(255, 202, 93, 0.4);
    margin: 0 auto;
    margin-top: 31rpx;
    border-radius: 20rpx 20rpx 0 0;
    overflow: auto;
}

.timer .title {
    text-align: center;
    font-family: PingFang SC;
    font-size: 28rpx;
    line-height: 40rpx;
    letter-spacing: 0rpx;
    margin-top: 24rpx;
    color: #ffb54d;
}

.timer .list {
    margin: 16rpx 0 16rpx 18rpx;
}

.timer .chose {
    width: 114rpx;
    height: 88rpx;
    background-color: #ffcc66;
    box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(255, 202, 93, 0.4);
    border-radius: 16rpx;
}

.timer .item {
    width: 114rpx;
